#include<bits/stdc++.h>

using namespace std;

int main()
{
    
    int smallest=1e9, largest=-1e9;
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    cin>>arr[i];
    
    for(int i=0;i<n;i++)
    {
        if(arr[i]>largest)
        largest=arr[i];
        if(arr[i]<smallest)
        smallest=arr[i];
    }
    cout<<"Largest number is: "<<largest<<" and smallest is "<<smallest;
    
    return 0;
    
}
